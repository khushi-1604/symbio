var jq = $.noConflict();
if (jq(window).width() >= 767) {
  jq(window).on('beforeunload', function(){
    jq(window).scrollTop(0);
  });
};

jq(window).load(function () {
  // fixed_right_Menu hide on load START
  jq('.Prod_menuTab,.toggleRTsideMenu').fadeOut();
  // fixed_right_Menu hide on load CLOSE

  //jq(document).ready(function () {
  // 1 Preloader
  // $(".ngc_loader").css({ "display": 'none' });
  // 2 Preloader
  setTimeout(function(){
  	jq("#loader-wrapper").css({ "display": 'none' });
  }, 1500);
  
});

// wow js start
if (jq(window).width() >= 767) {
  jq(window).on('load', function(){
    setTimeout(function(){new WOW().init();});  
  });
}
// wow js end

//########################## 1 window_scroll START  ##########################

// Onscroll_fixedMenu_animate START
if (jq(window).width() >= 767) {

  jq(window).scroll(function() {
    var sec1height = jq(".section1").innerHeight(); 
    var fixedRtmenuheight = jq(".Prod_menuTab").innerHeight();
    var getfixedheight = fixedRtmenuheight/2;
    // var stickymenu = jq(".social_sticky_container").innerHeight();
    // var getstickymenuheight = stickymenu/2;
    if ( (jq(this).scrollTop()>= sec1height - getfixedheight ) ) {
    // if ( (jq(this).scrollTop()>= sec1height - getfixedheight ) ||  (jq(this).scrollTop()>= sec1height - getstickymenuheight ) ) {
      jq('.Prod_menuTab,.toggleRTsideMenu').fadeIn(600);
    }
    else {
      jq('.Prod_menuTab,.toggleRTsideMenu').fadeOut(100);
    }
  });

  // on_scroll_to_footer_fixed_menu_hide_Start
  jq(window).scroll(function() {
    var footer_height = parseFloat( jq("footer").innerHeight() );  
    var sitemap_height = parseFloat( jq(".section_Sitemap").innerHeight() );   
    var getfooter_sitemap = footer_height + sitemap_height;
    var threshold = getfooter_sitemap; // number of pixels before bottom of page that you want to start fading
    var Tabmenu_opacity = ((jq(document).height() - jq(window).height()) - jq(window).scrollTop()) / threshold;
    if( Tabmenu_opacity <= 0 ) {
      jq(".Prod_menuTab").hide();
    }
    // else {
    //   jq(".Prod_menuTab").show();
    // }
    jq(".Prod_menuTab").css("opacity", Tabmenu_opacity ); 
  });
  // on_scroll_to_footer_fixed_menu_hide_Close

};
// Onscroll_fixedMenu_animate CLOSE


