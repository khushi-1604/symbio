jq('.customselect1').each(function() {
  var $this = jq(this), numberOfOptions = jq(this).children('option').length;

  $this.addClass('select1-hidden'); 
  $this.wrap('<div class="select1"></div>');
  $this.after('<div class="select1-styled"></div>');

  var $styledSelect = $this.next('div.select1-styled');
  $styledSelect.text($this.children('option').eq(0).text());

  var $list = jq('<ul />', {
      'class': 'select1-options'
  }).insertAfter($styledSelect);

  for (var i = 0; i < numberOfOptions; i++) {
    jq('<li />', {
        text: $this.children('option').eq(i).text(),
        rel: $this.children('option').eq(i).val(),
        value: $this.children('option').eq(i).val()
    }).appendTo($list);
  }

  var $listItems = $list.children('li');

  $styledSelect.click(function(e) {
      e.stopPropagation();
      jq('div.select1-styled.active').not(this).each(function(){
        jq(this).removeClass('active').next('ul.select1-options').hide();
      });
      jq(this).toggleClass('active').next('ul.select1-options').toggle();
  });

  $listItems.click(function(e) {
      e.stopPropagation();
      $styledSelect.text(jq(this).text()).removeClass('active');
      $this.val(jq(this).attr('rel'));
      $list.hide();
      //console.log($this.val());
  });

  jq(document).click(function() {
      $styledSelect.removeClass('active');
      $list.hide();
  });
});
